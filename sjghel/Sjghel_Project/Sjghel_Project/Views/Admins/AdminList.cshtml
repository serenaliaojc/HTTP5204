@model IEnumerable<Sjghel_Project.Models.Admin>

@{
    ViewBag.Title = "AdminList";
    Layout = "~/Views/Shared/_Layout_Admin.cshtml";
}

<h2>AdminList</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.admin_name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.admin_role)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.admin_name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.admin_role)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.admin_id }) |
            @Html.ActionLink("Details", "Details", new { id=item.admin_id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.admin_id })
        </td>
    </tr>
}

</table>
