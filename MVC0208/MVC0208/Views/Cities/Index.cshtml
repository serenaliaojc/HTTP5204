@model IEnumerable<MVC0208.Models.City>
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Cities</title>
</head>
<body>
    <h1>Cities</h1>
    <div> 
        @foreach (var item in Model)
        {
            <ul>
                <li>Name: @item.Name</li>
                <li>Population: @item.Population</li>
                <li>Mayor: @item.Mayor</li>
                <li>Country Code: @item.Country_Code</li>
            </ul>
        }
    </div>
    <div>
        @ViewBag.ErrorMessage
    </div>
</body>
</html>
